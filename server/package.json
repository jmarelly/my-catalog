{
  "name": "strauss-catalog-server",
  "version": "1.0.0",
  "description": "Product catalog API server",
  "main": "dist/index.js",
  "scripts": {
    "start": "npm run build && npm run start:prod",
    "build": "npx tsc",
    "start:dev": "npx rimraf dist && nodemon --env-file=.env --config nodemon.json src/index.ts",
    "start:prod": "node --env-file=.env dist/index.js",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/better-sqlite3": "^7.6.13",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/helmet": "^0.0.48",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.10.2",
    "@types/uuid": "^10.0.0",
    "drizzle-kit": "^0.31.8",
    "nodemon": "^3.1.7",
    "pino-pretty": "^13.1.3",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "better-sqlite3": "^12.5.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "drizzle-orm": "^0.45.0",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "pino": "^10.1.0",
    "uuid": "^11.0.3",
    "zod": "^4.1.13"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
